@model BookManagement1.Models.Categolory

@{
    ViewBag.Title = "Create";
    Layout = "~/Areas/Admin/Views/Shared/_Layout.cshtml";
}

@section header{
    Categolory page create
}

@using (Html.BeginForm()) 
{
    @Html.AntiForgeryToken()
     @Html.ValidationSummary(true)
        <div class="form-group">
            @Html.LabelFor(model => model.CategoloryName)
                @Html.TextBoxFor(model => model.CategoloryName, new { @class = "form-control", @placeholder = "Enter categolory"})
                @Html.ValidationMessageFor(model => model.CategoloryName)
        </div>

        <div class="form-group">
            <button type="submit" class="btn btn-primary btn-md">Submit Button</button>
        </div>
}

<div>
    @Html.ActionLink("Back to List", "Index")
</div>
